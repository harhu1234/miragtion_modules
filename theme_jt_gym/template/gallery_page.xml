<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="gallery_page" name="Gym Gallery Page">
        <t t-call="website.layout">
            <div class="oe_structure oe_empty">
                <!-- Breadcrumb Section Begin -->
                <section class="breadcrumb-section set-bg" data-setbg="/theme_jt_gym/static/src/img/breadcrumb-bg.jpg">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 text-center o_colored_level o_animate_preview o_animate o_anim_fade_in_up">
                                <div class="breadcrumb-text">
                                    <h2>Gallery</h2>
                                    <div class="bt-option">
                                        <a href="./">Home</a>
                                        <a href="#">Features</a>
                                        <span>Gallery</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- Breadcrumb Section End -->

                <!-- Gallery Section Begin -->
                <div class="gallery-section gallery-page">
                    <div class="gallery o_colored_level o_animate_preview o_animate o_anim_bounce_in">
                        <div class="grid-sizer"></div>
                        <div class="gs-item grid-wide set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-1.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-1.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-2.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-2.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-3.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-3.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-4.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-4.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-5.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-5.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item grid-wide set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-6.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-6.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item grid-wide set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-7.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-7.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-8.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-8.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                        <div class="gs-item set-bg" data-setbg="/theme_jt_gym/static/src/img/gallery/gallery-9.jpg">
                            <a href="/theme_jt_gym/static/src/img/gallery/gallery-9.jpg" class="thumb-icon image-popup"><i class="fa fa-picture-o"></i></a>
                        </div>
                    </div>
                </div>
                <!-- Gallery Section End -->

                <!-- Get In Touch Section Begin -->
                <div class="gettouch-section">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-4 o_colored_level o_animate_preview o_animate o_anim_fade_in_up">
                                <div class="gt-text">
                                    <i class="fa fa-map-marker"></i>
                                    <p>333 Middle Winchendon Rd, Rindge,<br/> NH 03461</p>
                                </div>
                            </div>
                            <div class="col-md-4 o_colored_level o_animate_preview o_animate o_anim_fade_in_up">
                                <div class="gt-text">
                                    <i class="fa fa-mobile"></i>
                                    <ul>
                                        <li>125-711-811</li>
                                        <li>125-668-886</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-md-4 o_colored_level o_animate_preview o_animate o_anim_fade_in_up">
                                <div class="gt-text email">
                                    <i class="fa fa-envelope"></i>
                                    <p>Support.gymcenter@gmail.com</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Get In Touch Section End -->
            </div>
        </t>
    </template>
</odoo>