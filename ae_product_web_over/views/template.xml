<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="aspl_assets_backend" name="ae_product_web_over Assets"
                  inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript"
                        src="/ae_product_web_over/static/src/js/abstract_controller_inh.js"/>
            </xpath>
        </template>

            <template name="Product Popover" id="aspl_product_popover_template">
            <div class="row" style="position:relative;">
                <div class="col-md-8">
                    <h3>
                        <t t-esc="product.display_name"/>
                    </h3>
                </div>
                <div class="col-md-4">
                    <span t-esc="product.image_128" t-options="{'widget':'image'}" t-if="product.image_128"/>
                </div>
                <div class="col-md-12">
                    <p style="margin:0px;">
                        <strong>Category :</strong>
                        <t t-esc="product.categ_id.name"/>
                    </p>
                    <p style="margin:0px;">
                        <strong>Sale Price :</strong>
                        <t t-esc="product.lst_price"/>
                    </p>
                    <p style="margin:0px;">
                        <strong>Cost :</strong>
                        <t t-esc="product.standard_price"/>
                    </p>
                    <p style="margin:0px;">
                        <strong>Internal Reference :</strong>
                        <t t-esc="product.default_code"/>
                    </p>
                    <p style="margin:0px;">
                        <strong>On Hand Qty :</strong>
                        <t t-esc="product.qty_available"/>
                    </p>
                </div>
            </div>
        </template>
    </data>
</odoo>