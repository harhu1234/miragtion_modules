<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="paperformat_student_info" model="report.paperformat">
	    <field name="name">Student information</field>
	    <field name="default" eval="True"/>
	    <field name="format">custom</field>
	    <field name="page_height">0</field>
	    <field name="page_width">0</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">40</field>
	    <field name="margin_bottom">25</field>
	    <field name="margin_left">7</field>
	    <field name="margin_right">7</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">35</field>
	    <!-- <field name="dpi">80</field> -->
	</record>

	
	<report id="report_student_info"
			string="Student information"
			model = "student.details"
			report_type="qweb-pdf"
			name="student_details.report_information"
			file="student_details.report_information"
			print_report_name="'Student Information - %s' % (object.name)"
			paperformat="student_details.paperformat_student_info"/>

	<report id="report_department_info"
			string="Department information"
			model = "department.details"
			report_type="qweb-pdf"
			name="student_details.report_information_department"
			file="student_details.report_information_department"
			print_report_name="'Department Information - %s' % (object.name)"
			paperformat="student_details.paperformat_student_info"/>

	
</odoo>