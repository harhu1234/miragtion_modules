<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="product_detail_list" inherit_id="website_sale.products">
		<xpath expr="//div[@id='products_grid']/div[hasclass('o_wsale_products_grid_table_wrapper')]" position="before">
            <div t-if="bins">
            <table class="table">
                <thead class="thead-dark">
                    <th style="text-align: center;"><span>Internal RF</span></th>
                    <th style="text-align: center;"><span>Description</span></th>
                    <th style="text-align: center;"><span>weight</span></th>
                    <th style="text-align: center;"><span>HSN</span></th>
                    <th style="text-align: center;"><span>weight</span></th>
                    <th style="text-align: center;"><span>HSN</span></th>
                    </thead>
                    <tbody>
                        <tr t-foreach="bins" t-as="tr_product">
                            <t t-foreach="tr_product" t-as="td_product">
                                <t t-if="td_product">
                                    <t t-set="product" t-value="td_product['product']" />
                                    <td>
                                        <tr>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.default_code"/>   
                                            </td>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.description_sale"/>
                                            </td>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.weight"/>
                                            </td>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.weight"/>
                                            </td>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.weight"/>
                                            </td>
                                            <td style="text-align: center;">
                                                <span class="h6" t-field="product.weight"/>
                                            </td>
                                        </tr>
                                    </td>
                                </t>
                            </t>
                        </tr>
                </tbody>
            </table>
            </div>
		</xpath>
	</template>
</odoo>