<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="gallery_page" name="Gallery Page">
		<t t-call="website.layout">
			<div class="oe_structure oe_empty">
				<section>
					<!-- bradcam_area -->
					<div class="bradcam_area">
					    <div class="single_bradcam  d-flex align-items-center bradcam_bg_3 overlay">
					        <div class="container">
					            <div class="row align-items-center justify-content-center">
					                <div class="col-xl-9 o_colored_level o_animate_preview o_animate o_anim_zoom_in">
					                    <div class="bradcam_text text-center">
					                        <h3>gallery</h3>
					                    </div>
					                </div>
					            </div>
					        </div>
					    </div>
					</div>
					<!-- bradcam_area end -->
				</section>
				<section>
					<!-- gallery_start -->
					<div class="gallery_area o_colored_level o_animate_preview o_animate o_anim_zoom_in">
					    <div class="single_gallery big_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/1.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/1.png" alt=""/>
					    </div>
					    <div class="single_gallery big_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/2.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/2.png" alt=""/>
					    </div>
					    <div class="single_gallery small_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/3.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/3.png" alt=""/>
					    </div>
					    <div class="single_gallery small_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/4.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/4.png" alt=""/>
					    </div>
					    <div class="single_gallery big_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/5.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/5.png" alt=""/>
					    </div>
					    <div class="single_gallery big_img">
					        <div class="hover_pop">
					            <a class="popup-image" href="/theme_ht_gym/static/src/img/gallery/6.png">
					            <i class="ti-plus"></i>
					            </a>
					        </div>
					        <img src="/theme_ht_gym/static/src/img/gallery/6.png" alt=""/>
					    </div>
					</div>
					<!-- gallery_area_end  -->
				</section>
				<section>
					<a href="#" class="view_pore boxed-btn3">View More</a>
				</section>
				<section>
					<!-- big_offer_area start  -->
					<div class="big_offer_area">
					    <div class="container">
					        <div class="row">
					            <div class="col-xl-12 o_colored_level o_animate_preview o_animate o_anim_zoom_in">
					                <div class="offter_text text-center">
					                    <h3>A Big Offer for <br/>
					                        This Summer
					                    </h3>
					                    <a href="#" class="boxed-btn3">Join Now</a>
					                </div>
					            </div>
					        </div>
					    </div>
					</div>
					<!-- big_offer_area end  -->
				</section>
			</div>
		</t>
	</template>
</odoo>