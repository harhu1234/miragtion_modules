<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="snippets" inherit_id="website.snippets" name="Octashop">
		<xpath expr="//div[@id='snippet_structure']" position="before">
			<div id="snippet_structure" class="o_panel">
				<div class="o_panel_header">Octashop</div>
				<div class="o_panel_body">
					<t t-snippet="theme_ht.ht_slide_snippet" t-thumbnail="/theme_ht/static/src/images/bg_1.jpg">
                    </t>
				</div>
			</div>
		</xpath>
	</template>

	<template id="ht_slide_snippet" name="Slide">
	</template>
</odoo>