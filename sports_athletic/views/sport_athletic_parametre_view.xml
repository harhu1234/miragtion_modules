                    <field name="num"/>
                    <field name="saison_id" invisible="1"/>
                    <field name="ligue_id"/>
                    <field name="situation_admin" />

                </tree>
            </field>
        </record>



        <record id="view_club_form" model="ir.ui.view">
            <field name="name">sports.club.form</field>
            <field name="model">sports.club</field>

            <field name="arch" type="xml">
                <form string="Ligue">
                    <sheet>

                        <field name="image" widget='image' class="oe_avatar" options='{"preview_image": "image_medium", "size": [90, 90]}'/>

                        <group>
                            <group>
                                <field name="name" required="1"/>
                                <field name="num" required="1"/>
                                <field name="code" required="1"/>
                                <field name="date" required="0"/>
                            </group>
                            <group>
                                <field name="name_ar" required="1"/>


                                <field name="saison_id" invisible="1"/>

                                <field name="ligue_id" required="1"/>
                                <field name="zone_id" invisible="1"/>
                                <field name="situation_admin" readonly="1" required="1"/>

                            </group>
                        </group>
                        <group>

                            <group>
                                <field name="region" />
                                <field name="commune" />
                                <field name="adresse"/>
                                <!--field name="id_old"/-->

                            </group>
                            <group>
                                <field name="contact"/>
                                <field name="mail"/>
                                <field name="tel"/>
                                <field name="gsm"/>
                                <field name="fax"/>

                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


