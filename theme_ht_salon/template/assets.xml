<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="salon_assets_website" name="Salon Management" inherit_id="website.assets_frontend">
		<xpath expr="//link[last()]" position="after">
			<!-- Css Styles -->
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/open-iconic-bootstrap.min.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/animate.css"/>

			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/owl.carousel.min.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/owl.theme.default.min.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/magnific-popup.css"/>

			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/aos.css"/>

			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/ionicons.min.css"/>

			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/bootstrap-datepicker.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/jquery.timepicker.css"/>


			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/flaticon.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/icomoon.css"/>
			<link rel="stylesheet" href="/theme_ht_salon/static/src/css/style.css"/>
		    <!-- Js Plugins -->
		    <script type="text/javascript">
    		$(document).ready(function () {
		        var url = window.location;
		        $('ul.navbar-nav a[href="'+ url +'"]').parent().addClass('active');
		        $('ul.navbar-nav a').filter(function() {
		             return this.href == url;
		        }).parent().addClass('active');
		    });
            </script>
<!--			<script src="/theme_ht_salon/static/src/js/jquery.min.js"></script>-->
			<script src="/theme_ht_salon/static/src/js/jquery-migrate-3.0.1.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/popper.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/bootstrap.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.easing.1.3.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.waypoints.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.stellar.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/owl.carousel.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.magnific-popup.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/aos.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.animateNumber.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/bootstrap-datepicker.js"></script>
			<script src="/theme_ht_salon/static/src/js/jquery.timepicker.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/scrollax.min.js"></script>
			<script src="/theme_ht_salon/static/src/js/main.js"></script>
		</xpath>
