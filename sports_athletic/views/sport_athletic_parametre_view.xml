<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>




        <menuitem name="Paramétre" id="base.menu_parametre_root" sequence="1"/>


        <menuitem name="Athlètes et Licences" id="base.menu_parametre2" sequence="1"  parent="base.menu_parametre_root" groups="sports_athletic.athlete_user"/>





        <!--Saison sportive' -->



        <record id="view_saison_tree" model="ir.ui.view">
            <field name="name">sports.saison.tree</field>
            <field name="model">sports.saison</field>

            <field name="arch" type="xml">
                <tree string="Saison sportive'">
                    <field name="name"/>
                    <field name="date_start"/>
                    <field name="date_end"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>


        <record id="view_saison_form" model="ir.ui.view">
            <field name="name">sports.saison.form</field>
            <field name="model">sports.saison</field>
            <field name="arch" type="xml">
                <form string="Saison sportive'">
                    <header>
                        <button name="action_active" string="Activer" type="object" states="future" class="btn-primary"/>
                        <button name="action_cloturee" string="Clôturer" type="object" states="active" class="btn-primary"/>
                        <button name="action_make_club_compliant" string="Rendre tous les clubs conforme" type="object" states="active" class="oe_highlight"/>
                        <field name="state" widget="statusbar" statusbar_visible="future,active,cloturee"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="date_start" required="1"/>
                                <field name="date_end" required="1"/>
                            </group>
                            <group>
                                <field name="name" required="1" attrs="{'readonly': [('state', 'in', ('active','cloturee'))]}"/>
                                <field name="make_club_non_compliant"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="action_saison" model="ir.actions.act_window">
            <field name="name">Saison sportive</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sports.saison</field>
            <field name="view_mode">tree,form</field>

        </record>

        <menuitem action="action_saison"
                  id="menu_action_saison" parent="base.menu_parametre2"
                  sequence="1" />






        <!--Ligue-->


        <record id="ligue_search_view" model="ir.ui.view">
            <field name="name">sports.ligue.search</field>
            <field name="model">sports.ligue</field>
            <field name="arch" type="xml">
                <search string="Ligue">
                    <field name="name"/>
                    <field name="name_ar"/>
                    <field name="num"/>

                    <field name="situation_admin" />
                    <field name="date"/>

                    <group  expand='0' string='Group by...'>
                        <filter string='Situation' name = "group_situation_admin" domain="[]" context="{'group_by' : 'situation_admin'}"/>

                    </group>
                </search>
            </field>
        </record>


        <record id="view_ligue_tree" model="ir.ui.view">
            <field name="name">sports.ligue.tree</field>
            <field name="model">sports.ligue</field>
            <field name="priority">2</field>
            <field name="arch" type="xml">
                <tree string="Ligue" >
                    <field name="name"/>
                    <field name="name_ar"/>
                    <field name="num"/>
                    <!--<field name="saison_id"/>-->
                    <field name="zone_id"/>
                    <field name="situation_admin" />
                    <field name="date"/>
                </tree>
            </field>
        </record>

        <record id="view_ligue_form" model="ir.ui.view">
            <field name="name">sports.ligue.form</field>
            <field name="model">sports.ligue</field>
            <field name="priority">2</field>
            <field name="arch" type="xml">
                <form string="Ligue">
                    <sheet>

                        <field name="image" widget='image' class="oe_avatar" options='{"preview_image": "image_medium", "size": [90, 90]}'/>

                        <group>
                            <group>
                                <field name="name" required="1"/>

                                <field name="num" required="1"/>
                                <field name="date" required="1"/>
                            </group>
                            <group>
                                <field name="name_ar" required="1"/>
                                <field name="saison_id" invisible="1"/>
                                <field name="zone_id"/>
                                <field name="situation_admin" readonly="1" required="1"/>

                            </group>
                        </group>
                        <group>

                            <group>
                                <field name="adresse"/>


                            </group>
                            <group>
                                <field name="mail"/>
                                <field name="tel"/>
                                <field name="fax"/>
                                <!--field name="id_old"/-->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_ligue" model="ir.actions.act_window">
            <field name="name">Ligue</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sports.ligue</field>
            <field name="view_mode">tree,form</field>
        </record>


        <menuitem action="action_ligue"
                  id="menu_action_ligue" parent="base.menu_parametre2"
                  sequence="2" />



        <!--club-->



        <record id="club_search_view" model="ir.ui.view">
            <field name="name">sports.club.search</field>
            <field name="model">sports.club</field>
            <field name="arch" type="xml">
                <search string="Club">
                    <field name="name"/>
                    <field name="name_ar"/>
                    <field name="num"/>
                    <field name="ligue_id"/>
                    <field name="situation_admin" />

                    <group  expand='0' string='Group by...'>
                        <filter string='Situation' name = "group_situation_admin" domain="[]" context="{'group_by' : 'situation_admin'}"/>
                        <filter string='Ligue' name = "group_ligue_id" icon="terp-mrp" domain="[]" context="{'group_by' : 'ligue_id'}"/>

                    </group>
                </search>
            </field>
        </record>


