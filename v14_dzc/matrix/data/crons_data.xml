<?xml version="1.0" encoding='UTF-8'?>

<odoo>
        <record id="mark_opportunity_won_cron" model="ir.cron">
                <field name="name">Auto Mark Won Opportunity</field>
                <field name="model_id" ref="model_crm_lead"/>
                <field name="state">code</field>
                <field name="code">model.mark_opportunity_won()</field>
                <field name="interval_number">1</field>
                <field name="interval_type">hours</field>
                <field name="numbercall">-1</field>
                <field name="doall" eval="False"/>
        </record>

        <record id="change_applicants_email_cron" model="ir.cron">
                <field name="name">Change Applicants Email Cron</field>
                <field name="model_id" ref="model_hr_applicant"/>
                <field name="state">code</field>
                <field name="code">model.change_applicants_email_cron()</field>
                <field name="interval_number">2</field>
                <field name="interval_type">minutes</field>
                <field name="numbercall">-1</field>
                <field name="doall" eval="False"/>
        </record>

        <record id="archive_applicants_cron" model="ir.cron">
                <field name="name">Auto Delete Applicants Cron</field>
                <field name="model_id" ref="model_hr_applicant"/>
                <field name="state">code</field>
                <field name="code">model.auto_delete_applic()</field>
                <field name="interval_number">1</field>
                <field name="interval_type">days</field>
                <field name="numbercall">-1</field>
                <field name="doall" eval="False"/>
        </record>
</odoo>