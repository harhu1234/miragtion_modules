<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Survey Invite Form View -->
        <record id="survey_invite_view_form_inh_feedback" model="ir.ui.view">
            <field name="name">survey.invite.view.form</field>
            <field name="model">survey.invite</field>
            <field name="inherit_id" ref="survey.survey_invite_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='survey_id']" position="replace">
                    <field name="survey_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>